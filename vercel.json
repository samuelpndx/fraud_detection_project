{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "includeFiles": "templates/**"
    },
    "app-models.py": {
      "includeFiles": "models/**"
    },
    "app-req.py": {
      "includeFiles": "requirements.txt"
    }
  }
}