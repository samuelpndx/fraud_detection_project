{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "includeFiles": [
      "app.py",
      "templates/**",
      "models/**",
      "requirements.txt"
    ]
  }
}